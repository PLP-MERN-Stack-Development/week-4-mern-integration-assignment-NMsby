{
  "name": "week-4-mern-integration-assignment-nmsby",
  "version": "1.0.0",
  "description": "MERN Stack Blog Application - Week 4 Assignment",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "server:start": "cd server && npm start",
    "client:build": "cd client && npm run build",
    "client:preview": "cd client && npm run preview",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "clean": "rm -rf node_modules server/node_modules client/node_modules",
    "test": "cd server && npm test && cd ../client && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PLP-MERN-Stack-Development/week-4-mern-integration-assignment-NMsby.git"
  },
  "keywords": [
    "mern",
    "mongodb",
    "express",
    "react",
    "nodejs",
    "blog",
    "fullstack"
  ],
  "author": "Nelson Masbayi",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
